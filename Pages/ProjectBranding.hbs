<style>
    /* General Styles */
body {
  font-family: 'Arial', sans-serif;
  margin: 0;
  padding: 0;
  line-height: 1.6;
  color: #333;
}

.container {
  width: 90%;
  max-width: 1200px;
  margin: 0 auto;
}

/* Header */
header {
  background: #333;
  color: #fff;
  padding: 20px 0;
}

header .logo h1 {
  margin: 0;
  font-size: 24px;
}

header .logo span {
  color: #ff6f61;
}

nav ul {
  list-style: none;
  padding: 0;
  margin: 0;
  display: flex;
  gap: 20px;
}

nav ul li a {
  color: #fff;
  text-decoration: none;
  font-weight: bold;
}

/* Hero Section */
.hero {
  background: linear-gradient(90deg, #ff6f61, #e65a50);
  padding: 100px 0;
  text-align: center;
  color: #fff;
}

.hero h1 {
  font-size: 48px;
  margin-bottom: 20px;
}

.hero p {
  font-size: 18px;
  margin-bottom: 30px;
}

.cta-btn {
  background: #fff;
  color: #ff6f61;
  padding: 10px 20px;
  text-decoration: none;
  border-radius: 5px;
  font-weight: bold;
}

.cta-btn:hover {
  background: #f4f4f4;
}

/* Features Section */
.features {
  padding: 20px 0;
  background: #fff;
}

.features h2 {
  text-align: center;
  margin-bottom: 40px;
  font-size: 36px;
}

.feature-grid {
  display: grid;
  grid-template-columns: repeat(auto-fit, minmax(300px, 1fr));
  gap: 20px;
}

.feature-card {
  background: #f9f9f9;
  padding: 20px;
  border-radius: 10px;
  text-align: center;
  box-shadow: 0 4px 6px rgba(0, 0, 0, 0.1);
}

.feature-card i {
  font-size: 40px;
  color: #ff6f61;
  margin-bottom: 20px;
}

.feature-card h3 {
  font-size: 24px;
  margin-bottom: 10px;
}

.feature-card p {
  font-size: 16px;
  color: #666;
}

/* Testimonials Section */
.testimonials {
  padding: 80px 0;
  background: #f4f4f4;
}

.testimonials h2 {
  text-align: center;
  margin-bottom: 40px;
  font-size: 36px;
}

.testimonial-grid {
  display: grid;
  grid-template-columns: repeat(auto-fit, minmax(300px, 1fr));
  gap: 20px;
}

.testimonial-card {
  background: #fff;
  padding: 20px;
  border-radius: 10px;
  text-align: center;
  box-shadow: 0 4px 6px rgba(0, 0, 0, 0.1);
}

.testimonial-card p {
  font-size: 16px;
  color: #666;
}

.testimonial-card h4 {
  font-size: 18px;
  margin-top: 10px;
}

/* Team Section */
.team {
  padding: 80px 0;
  background: #fff;
}

.team h2 {
  text-align: center;
  margin-bottom: 40px;
  font-size: 36px;
}

.team-grid {
  display: grid;
  grid-template-columns: repeat(auto-fit, minmax(250px, 1fr));
  gap: 20px;
}

.team-card {
  background: #f9f9f9;
  padding: 20px;
  border-radius: 10px;
  text-align: center;
  box-shadow: 0 4px 6px rgba(0, 0, 0, 0.1);
}

.team-card img {
  border-radius: 50%;
  margin-bottom: 20px;
}

.team-card h3 {
  font-size: 24px;
  margin-bottom: 10px;
}

.team-card p {
  font-size: 16px;
  color: #666;
}

/* Pricing Section */
.pricing {
  padding: 80px 0;
  background: #f4f4f4;
}

.pricing h2 {
  text-align: center;
  margin-bottom: 40px;
  font-size: 36px;
}

.pricing-grid {
  display: grid;
  grid-template-columns: repeat(auto-fit, minmax(300px, 1fr));
  gap: 20px;
}

.pricing-card {
  background: #fff;
  padding: 20px;
  border-radius: 10px;
  text-align: center;
  box-shadow: 0 4px 6px rgba(0, 0, 0, 0.1);
}

.pricing-card h3 {
  font-size: 24px;
  margin-bottom: 20px;
}

.pricing-card .price {
  font-size: 36px;
  color: #ff6f61;
  margin-bottom: 20px;
}

.pricing-card .price span {
  font-size: 16px;
  color: #666;
}

.pricing-card ul {
  list-style: none;
  padding: 0;
  margin-bottom: 20px;
}

.pricing-card ul li {
  font-size: 16px;
  color: #666;
  margin-bottom: 10px;
}

/* Newsletter Section */
.newsletter {
  padding: 80px 0;
  background: #333;
  color: #fff;
  text-align: center;
}

.newsletter h2 {
  font-size: 36px;
  margin-bottom: 20px;
}

.newsletter p {
  font-size: 18px;
  margin-bottom: 30px;
}

.newsletter form {
  display: flex;
  justify-content: center;
  gap: 10px;
}

.newsletter input {
  padding: 10px;
  border: none;
  border-radius: 5px;
  width: 300px;
}

.newsletter button {
  background: #ff6f61;
  color: #fff;
  padding: 10px 20px;
  border: none;
  border-radius: 5px;
  cursor: pointer;
}

.newsletter button:hover {
  background: #e65a50;
}

/* Footer */
footer {
  background: #333;
  color: #fff;
  text-align: center;
  padding: 20px 0;
}

footer p {
  margin: 0;
}

.social-links {
  margin-top: 10px;
}

.social-links a {
  color: #fff;
  margin: 0 10px;
  font-size: 20px;
}

.social-links a:hover {
  color: #ff6f61;
}
</style>
<script>
